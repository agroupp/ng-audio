{
  "version": 2,
  "generators": {
    "@nrwl/angular": {
      "application": {
        "strict": true,
        "style": "scss",
        "prefix": "grp",
        "linter": "eslint",
        "unitTestRunner": "jest",
        "e2eTestRunner": "cypress"
      },
      "library": {
        "strict": true,
        "style": "scss",
        "prefix": "grp",
        "linter": "eslint",
        "unitTestRunner": "jest"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress",
      "strict": true
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest",
      "strict": true
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "audio-demo": {
      "projectType": "application",
      "root": "apps/audio-demo",
      "sourceRoot": "apps/audio-demo/src",
      "prefix": "grp",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/audio-demo",
            "index": "apps/audio-demo/src/index.html",
            "main": "apps/audio-demo/src/main.ts",
            "polyfills": "apps/audio-demo/src/polyfills.ts",
            "tsConfig": "apps/audio-demo/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/audio-demo/src/favicon.ico",
              "apps/audio-demo/src/assets"
            ],
            "styles": [
              "apps/audio-demo/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/audio-demo/src/environments/environment.ts",
                  "with": "apps/audio-demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "audio-demo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "audio-demo:build:production"
            }
          }
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "audio-demo:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/audio-demo/src/**/*.ts",
              "apps/audio-demo/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/audio-demo"
          ],
          "options": {
            "jestConfig": "apps/audio-demo/jest.config.js",
            "passWithNoTests": false
          }
        }
      }
    },
    "audio-demo-e2e": {
      "root": "apps/audio-demo-e2e",
      "sourceRoot": "apps/audio-demo-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/audio-demo-e2e/cypress.json",
            "tsConfig": "apps/audio-demo-e2e/tsconfig.e2e.json",
            "devServerTarget": "audio-demo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "audio-demo:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/audio-demo-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "core": {
      "projectType": "library",
      "root": "packages/core",
      "sourceRoot": "packages/core/src",
      "prefix": "grp",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:package",
          "options": {
            "tsConfig": "packages/core/tsconfig.lib.json",
            "project": "packages/core/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "packages/core/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/core/src/**/*.ts",
              "packages/core/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/packages/core"
          ],
          "options": {
            "jestConfig": "packages/core/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "audio-demo"
}
